{
  "dashboard": {
    "id": null,
    "uid": "vritti-skills-gating",
    "title": "Vritti - Skills & Pre-Action Gating",
    "description": "Skills library monitoring, promotion tracking, and pre-action gating decisions",
    "tags": ["vritti", "skills", "gating", "promotion"],
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Total Skills",
        "description": "Active promoted skills in library",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(episodic_memory_promoted_skills_total)",
            "legendFormat": "Skills",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value"
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Promotions (24h)",
        "description": "Episodes promoted to skills today",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(increase(episodic_memory_skill_promotions_total[24h]))",
            "legendFormat": "Promotions",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Skill Applications (24h)",
        "description": "Times skills were applied",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(increase(episodic_memory_skill_applications_total[24h]))",
            "legendFormat": "Applications",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Skill Success Rate",
        "description": "Overall skill application success rate",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(episodic_memory_skill_applications_total{success=\"true\"}[24h])) / sum(rate(episodic_memory_skill_applications_total[24h]))",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.8, "color": "yellow"},
                {"value": 0.9, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "type": "timeseries",
        "title": "Gating Decisions Over Time",
        "description": "Pre-action gating recommendations",
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (recommendation) (rate(episodic_memory_gating_decision_total[5m]))",
            "legendFormat": "{{recommendation}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "calcs": ["sum"]}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 30,
              "stacking": {"mode": "normal"}
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "block"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]
            },
            {
              "matcher": {"id": "byName", "options": "rewrite"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}]
            },
            {
              "matcher": {"id": "byName", "options": "hint"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "yellow"}}]
            },
            {
              "matcher": {"id": "byName", "options": "proceed"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]
            }
          ]
        }
      },
      {
        "id": 6,
        "type": "piechart",
        "title": "Gating Decisions Distribution (24h)",
        "gridPos": {"x": 12, "y": 4, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "sum by (recommendation) (increase(episodic_memory_gating_decision_total[24h]))",
            "legendFormat": "{{recommendation}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"},
          "pieType": "donut",
          "displayLabels": ["percent"]
        },
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "block"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]
            },
            {
              "matcher": {"id": "byName", "options": "rewrite"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}]
            },
            {
              "matcher": {"id": "byName", "options": "hint"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "yellow"}}]
            },
            {
              "matcher": {"id": "byName", "options": "proceed"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]
            }
          ]
        }
      },
      {
        "id": 7,
        "type": "stat",
        "title": "Failures Prevented",
        "description": "BLOCK + REWRITE decisions (estimated prevented failures)",
        "gridPos": {"x": 18, "y": 4, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(increase(episodic_memory_gating_decision_total{recommendation=~\"block|rewrite\"}[24h]))",
            "legendFormat": "Prevented",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "type": "stat",
        "title": "Gating P95 Latency",
        "description": "Pre-action gating response time",
        "gridPos": {"x": 18, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(episodic_memory_gating_latency_seconds_bucket[5m])) * 1000",
            "legendFormat": "P95",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "type": "timeseries",
        "title": "Gating Confidence Distribution",
        "description": "Confidence scores for gating decisions",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.5, sum by (recommendation, le) (rate(episodic_memory_gating_confidence_bucket[5m])))",
            "legendFormat": "{{recommendation}} (median)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.9, sum by (recommendation, le) (rate(episodic_memory_gating_confidence_bucket[5m])))",
            "legendFormat": "{{recommendation}} (P90)",
            "refId": "B"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 10,
        "type": "timeseries",
        "title": "Matched Failures per Gating Request",
        "gridPos": {"x": 12, "y": 12, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(episodic_memory_gating_matched_failures_bucket[5m]))",
            "legendFormat": "Median",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(episodic_memory_gating_matched_failures_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 11,
        "type": "timeseries",
        "title": "Matched Skills per Gating Request",
        "gridPos": {"x": 18, "y": 12, "w": 6, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(episodic_memory_gating_matched_skills_bucket[5m]))",
            "legendFormat": "Median",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(episodic_memory_gating_matched_skills_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 12,
        "type": "timeseries",
        "title": "Skill Promotion Rate",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (error_class) (rate(episodic_memory_skill_promotions_total[1h]))",
            "legendFormat": "{{error_class}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "bars"
            }
          }
        }
      },
      {
        "id": 13,
        "type": "table",
        "title": "Top Skills by Success Rate",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "topk(10, episodic_memory_skill_success_rate)",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "skill_id": false},
              "renameByName": {
                "skill_id": "Skill ID",
                "skill_name": "Skill Name",
                "Value": "Success Rate"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "left"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Success Rate"},
              "properties": [
                {"id": "unit", "value": "percentunit"},
                {"id": "thresholds", "value": {
                  "mode": "absolute",
                  "steps": [
                    {"value": 0, "color": "red"},
                    {"value": 0.8, "color": "yellow"},
                    {"value": 0.9, "color": "green"}
                  ]
                }}
              ]
            }
          ]
        }
      },
      {
        "id": 14,
        "type": "timeseries",
        "title": "Fix Validations",
        "description": "Fix validation outcomes (success vs still_failed)",
        "gridPos": {"x": 0, "y": 28, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "sum by (outcome) (rate(episodic_memory_fix_validations_total[5m]))",
            "legendFormat": "{{outcome}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "success"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]
            },
            {
              "matcher": {"id": "byName", "options": "still_failed"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]
            }
          ]
        }
      },
      {
        "id": 15,
        "type": "stat",
        "title": "Fix Success Rate",
        "description": "Overall fix validation success rate",
        "gridPos": {"x": 12, "y": 28, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "sum(rate(episodic_memory_fix_validations_total{outcome=\"success\"}[24h])) / sum(rate(episodic_memory_fix_validations_total[24h]))",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.85, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 16,
        "type": "stat",
        "title": "Promotion Duration (avg)",
        "description": "Average time to check and promote skill",
        "gridPos": {"x": 18, "y": 28, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(episodic_memory_skill_promotion_duration_seconds_bucket[1h]))",
            "legendFormat": "Median",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        }
      }
    ]
  }
}
